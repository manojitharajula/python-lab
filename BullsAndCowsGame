import random
num=input("Enter your 4-digit number(Digits Must Be different):")
cows=0
bulls=0
temp=random.sample(range(0,9),4)
if(num==temp):
    bulls=4
else:
    for i in range(0,4):
        if(int(num[i]) in temp):
            if(int(num[i])==temp[i]):
                bulls+=1
            else:                
                cows+=1          
print("My Guess:  {} \nYour num:  {}\nSo, You have scored {} bulls and {} cows".format(''.join(map(str,temp)),num,bulls,cows))         
           
